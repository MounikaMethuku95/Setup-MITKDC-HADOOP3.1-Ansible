[libdefaults]
  default_realm = {{ realm }}
  dns_lookup_realm = false
  dns_lookup_kdc = false
  ticket_lifetime = 24h
  renew_lifetime = 7d
  forwardable = true

[realms]
  {{ realm }} = {
    kdc = localhost
    admin_server = localhost
  }

[domain_realm]
  .hadoop.test = {{ realm }}
  hadoop.test = {{ realm }}
